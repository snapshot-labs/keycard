{
  "name": "keycard-api",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "lint": "eslint src/ test/ --ext .ts,.json",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/src/index.js",
    "start:test": "dotenv -e test/.env.test yarn dev",
    "test": "PORT=3077 start-server-and-test 'yarn start:test' 3077 'dotenv -e test/.env.test jest --runInBand'",
    "test:e2e": "PORT=3077 start-server-and-test 'yarn start:test' 3077 'dotenv -e test/.env.test jest --runInBand --collectCoverage=false test/e2e/'"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "yarn lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": "@snapshot-labs"
  },
  "prettier": "@snapshot-labs/prettier-config",
  "dependencies": {
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@snapshot-labs/snapshot-metrics": "^1.1.0",
    "@snapshot-labs/snapshot-sentry": "^1.4.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "nodemon": "^3.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@snapshot-labs/eslint-config": "^0.1.0-beta.11",
    "@snapshot-labs/prettier-config": "^0.1.0-beta.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.4.8",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-node-single-context": "^29.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.1",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
